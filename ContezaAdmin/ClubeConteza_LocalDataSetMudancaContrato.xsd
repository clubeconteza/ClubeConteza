                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT TB012_Contratos.TB012_id, TB012_Contratos.TB012_Edicao, RTRIM(Titular.TB013_NomeCompleto) AS TitularNomeCompleto, Titular.TB013_CPFCNPJ AS TitularCPF, 
                         RTRIM(Titular.TB013_RG) AS TitularRG, RTRIM(Titular.TB013_RGOrgaoEmissor) AS TitularOrgaoEmissor, Dependente.TB013_id AS DependenteId, RTRIM(Dependente.TB013_NomeCompleto) 
                         AS DependenteNomeCompleto, Dependente.TB013_DataNascimento AS DependenteDataNascimento, 
                         CASE Dependente.TB013_Status WHEN 0 THEN 'INCLUIR' WHEN 1 THEN 'NÃO ALTERAR' WHEN 3 THEN 'CANCELAR' END AS DependenteAcao
FROM            TB012_Contratos INNER JOIN
                         TB013_Pessoa AS Titular ON TB012_Contratos.TB013_id = Titular.TB013_id INNER JOIN
                         TB013_Pessoa AS Dependente ON TB012_Contratos.TB012_id = Dependente.TB012_id AND Titular.TB013_id &lt;&gt; Dependente.TB013_id
WHERE        (TB012_Contratos.TB012_id = @TB012_id) AND (TB012_Contratos.TB012_Edicao = 1)
ORDER BY DependenteAcao, DependenteNomeCompleto</CommandText>
